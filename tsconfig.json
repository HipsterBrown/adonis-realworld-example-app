{
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "build",
    "cypress"
  ],
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": [
      "ES2022"
    ],
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "incremental": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "declaration": false,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "outDir": "build",
    "rootDir": "./",
    "sourceMap": true,
    "paths": {
      "@/": [
        "./"
      ],
      "#controllers/*": [
        "./app/controllers/*.js"
      ],
      "#exceptions/*": [
        "./app/exceptions/*.js"
      ],
      "#models/*": [
        "./app/models/*.js"
      ],
      "#services/*": [
        "./app/services/*.js"
      ],
      "#listeners/*": [
        "./app/listeners/*.js"
      ],
      "#events/*": [
        "./app/events/*.js"
      ],
      "#middleware/*": [
        "./app/middleware/*.js"
      ],
      "#validators/*": [
        "./app/validators/*.js"
      ],
      "#start/*": [
        "./start/*.js"
      ],
      "#config/*": [
        "./config/*.js"
      ],
      "#app/*": [
        "./app/*.js"
      ],
      "#database/*": [
        "./database/*.js"
      ],
      "#contracts/*": [
        "./contracts/*.js"
      ]
    },
    "types": [],
    "strictPropertyInitialization": false
  },
  "extends": "@adonisjs/tsconfig/tsconfig.app.json"
}