{
  "name": "adonis-realworld-example-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch --no-assets",
    "build": "node ace build --production --no-assets",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "db:setup": "node ace migration:run && node ace db:seed",
    "test": "node ace test",
    "test:e2e": "node ace test e2e"
  },
  "eslintConfig": {
    "extends": [
      "@adonisjs/eslint-config/app"
    ],
    "rules": {
      "@typescript-eslint/explicit-member-accessibility": "off",
      "unicorn/filename-case": "off",
      "@typescript-eslint/no-shadow": "off"
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": "@adonisjs/prettier-config",
  "devDependencies": {
    "@adonisjs/assembler": "^7.1.0",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@faker-js/faker": "^7.5.0",
    "@japa/browser-client": "^2.0.2",
    "@japa/plugin-adonisjs": "^3.0.0",
    "@japa/runner": "^3.1.1",
    "@swc/core": "^1.3.105",
    "@types/luxon": "^3.4.2",
    "@types/marked": "^4.0.6",
    "@types/sanitize-html": "^2.6.2",
    "eslint": "^8.56.0",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.32.2",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "~4.8.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^6.2.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^19.0.0",
    "@adonisjs/lucid-slugify": "^2.2.1",
    "@adonisjs/session": "^7.1.1",
    "@adonisjs/shield": "^8.1.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/validator": "^12.6.0",
    "argon2": "^0.31.2",
    "edge.js": "^6.0.1",
    "luxon": "^3.3.0",
    "marked": "^4.3.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.10.0",
    "sqlite3": "^5.1.6"
  },
  "type": "module"
}